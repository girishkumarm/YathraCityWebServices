<?xml version="1.0"?>
<schema package="com.yathraCity.core" name="Core Package">

	<Class name="CouponDetails"
		description="car avaliable on that particular day"
		displayorder="0">
		<property name="coupon" type="string" description="coupens name" />
		<property name="fromDate" type="string" description="valid from" />
		<property name="toDate" type="string" description="to from" />
	</Class>
	
	<Class name="ListOfCouponsUsed"
		description="LoginResponse which describes the user is real or not"
		displayorder="0">
		<Array name="listOfCoupons" valueClass="CouponDetails" description="list of coupens" />
	</Class>
</schema>
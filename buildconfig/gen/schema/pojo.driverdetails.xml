<?xml version="1.0"?>
<schema package="com.yathraCity.core" name="Core Package">
	<Class name="RegisterCarDetails"
		description="input pojo which describes all the required information of the user"
		displayorder="0">
		<property name="carName" type="String" description="user name" />
		<property name="carModel" type="String" description="car model" />
		<property name="carNumber" type="String" description="car number" />
		<property name="maximumPeople" type="int" description="maximum no of people" />
		<property name="driverName" type="String" description="name of the driver" />
		<property name="phoneNumber" type="long" description="phone number of the deriver"/>
		<property name="cityName" type="String" description="city of car present"/>
		<property name="minimumDistance" type="int" description="minimum distance to be paid"/>
		<property name="pricePerKm" type="int" description="price per km"/>
		<property name="driverLicence" type="String" description="licence of the driver"/>
	</Class>

	<Class name="RegisterCarResponse" description="RegisterCarResponse" displayorder="0">
		<property name="message" type="String" description="message which describes the failure/success" />
	</Class>
</schema>
	
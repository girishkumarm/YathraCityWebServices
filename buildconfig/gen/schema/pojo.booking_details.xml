<?xml version="1.0"?>
<schema package="com.yathraCity.core" name="Core Package">
	<Class name="RegisterBookingInput" description="input to book a car"
		displayorder="0">
		<property name="userName" type="String" description="userName" />
		<property name="emailId" type="String" description="emailId" />
		<property name="phoneNumber" type="long" description="phoneNumber" />
		<property name="numberOfPeoples" type="String" description="numberOfPeoples" />
		<property name="bookedCarName" type="string" description="bookedCarName" />
		<property name="carBookedAt" type="long" description="carBookedAt" />
		<property name="pickUpPoint" type="int" description="pickUpPoint" />
		<property name="destination" type="String" description="destination" />
		<property name="numberOfDays" type="int" description="numberOfDays" />
		<property name="bookedCarNumber" type="String" description="bookedCarNumber" />
		<property name="bookedCarOwner" type="String" description="bookedCarOwner" />
		<property name="discountOffered" type="int" description="discountOffered" />
		<property name="bookingConfirmation" type="boolean"
			description="whether the booking is confirmed or not" />
	</Class>


</schema>
	